{
  "name": "ts-type-visitor",
  "version": "0.2.0",
  "main": "dist/index.js",
  "module": "module/index.js",
  "author": "Saito Nakamura <michael.bashurov@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "npm run lint",
    "watch": "tsc --watch --project ./tsconfig.json",
    "build": "tsc --project ./tsconfig.json",
    "build:module": "tsc --project ./tsconfig.module.json",
    "lint": "tsc --project ./tsconfig.json --noEmit",
    "prepublishOnly": "npm test && npm run build && npm run build:module"
  },
  "keywords": [
    "typescript",
    "typescript-transformer",
    "typescript-compiler-api"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ts-type-makeup/ts-type-visitor.git"
  },
  "bugs": {
    "url": "https://github.com/ts-type-makeup/ts-type-visitor/issues"
  },
  "homepage": "https://github.com/ts-type-makeup/ts-type-visitor#readme",
  "peerDependencies": {
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "prettier": "^2.2.0",
    "typescript": "^4.1.2"
  }
}
